from abc import ABC,abstractmethod

class vehicle(ABC):
    @abstractmethod
    def engine_capacity(self):
        return

    def typeofvehicle(self):
        return'Sedan'

class Audi(vehicle):

    def engine_capacity(Audi):
        return '2250HP'

    def typeofvehicle(Audi):
        return 'Sedan'


class BMW(vehicle):

    def engine_capacity(BMW):
        return '3250HP'
   # def typeofvehicle(BMW):
       # return

v1 = Audi()
v2 = BMW()

print(v1.engine_capacity())
print(v1.typeofvehicle())

print(v2.engine_capacity())
print(v2.typeofvehicle())